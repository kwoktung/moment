{
  "name": "moment",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack -p 4000",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --noEmit",
    "lint": "next lint && yarn type-check",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest run",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv ./cloudflare-env.d.ts",
    "db:generate": "drizzle-kit generate",
    "db:generate:custom": "drizzle-kit generate --custom",
    "db:add-custom": "./scripts/add-custom-migrations.sh",
    "db:regenerate": "rm -rf src/database/migrations && yarn db:generate && yarn db:add-custom",
    "db:migrate": "wrangler d1 migrations apply moment",
    "db:migrate:remote": "wrangler d1 migrations apply moment --remote",
    "db:reset": "wrangler d1 migrations reset moment",
    "api:generate": "rm -rf openapi.json && wget http://localhost:4000/api/docs -O openapi.json && rm -rf src/lib/api-client && openapi --input ./openapi.json --output ./src/lib/api-client --name ApiClient",
    "secrets:bulk": "wrangler secret bulk .env.production"
  },
  "dependencies": {
    "@emoji-mart/data": "^1.2.1",
    "@emoji-mart/react": "^1.1.1",
    "@hono/zod-openapi": "^1.1.0",
    "@hookform/resolvers": "^5.2.2",
    "@opennextjs/cloudflare": "^1.3.0",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@scalar/hono-api-reference": "^0.9.13",
    "@tanstack/react-query": "^5.90.12",
    "@tanstack/react-query-devtools": "^5.91.1",
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "bignumber.js": "^9.3.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.44.4",
    "embla-carousel-react": "^8.6.0",
    "emoji-mart": "^5.6.0",
    "hono": "^4.9.2",
    "jose": "^5.9.6",
    "lucide-react": "^0.539.0",
    "next": "15.4.8",
    "next-themes": "^0.4.6",
    "react": "19.1.2",
    "react-dom": "19.1.2",
    "react-easy-crop": "^5.5.6",
    "react-hook-form": "^7.68.0",
    "tailwind-merge": "^3.3.1",
    "vaul": "^1.1.2",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^24.3.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv": "^17.2.1",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9",
    "eslint-config-next": "15.4.8",
    "eslint-plugin-drizzle": "^0.2.3",
    "openapi-typescript-codegen": "^0.29.0",
    "prettier": "3.6.2",
    "tailwindcss": "^4",
    "tsx": "^4.20.4",
    "tw-animate-css": "^1.3.7",
    "typescript": "^5",
    "vitest": "^4.0.15",
    "wrangler": "^4.56.0"
  }
}
